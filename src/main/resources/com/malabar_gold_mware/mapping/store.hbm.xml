<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package = "com.qburst.malabar_gold_mware.mapping" >
 
    
    <class name="store"  table="store">
 
        <id name="id">
            <generator class="native"/>
        </id>
 
        <property name="name" type="string" column="name"/>
                <property name="country" type="string" column="country"/>
        
                <property name="city" type="string" column="city"/>
        
                <property name="state" type="string" column="state"/>
                <property name="location" type="string" column="location"/>
                <property name="address" type="string" column="address"/>
                <property name="latitude" type="float" column="latitude"/>
                <property name="longitude" type="float" column="longitude"/>
             
       
    </class>
 
</hibernate-mapping>